    "css-loader": "^0.28.4",
    "dat.gui": "^0.7.2",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "^4.0.3",
    "glslify": "^6.1.1",
    "glslify-loader": "^1.0.2",
    "node-sass": "^4.13.1",
    "raw-loader": "^0.5.1",
    "recursive-readdir": "^2.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.1",
    "webpack-dev-server": "^2.7.0"
