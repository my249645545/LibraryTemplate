{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run html:watch & NODE_ENV=development webpack-dev-server --open & node ./tasks/watch-asset.js & node ./tasks/shader-watcher.js & npm run html:watch",
    "start:windows": "concurrently \"set NODE_ENV=development&&webpack-dev-server --open\" \"npm run html:watch\" \"npm run shader\" \"npm run assets\"",
    "build:dev": "NODE_ENV=preprod npm run html & webpack --progress --profile --colors",
    "build": "NODE_ENV=production npm run html & NODE_ENV=production webpack --progress --profile --colors",
    "watch": "webpack --watch",
    "html": "node ./tasks/html.js",
    "assets": "node ./tasks/watch-asset.js",
    "shader": " node ./tasks/shader-watcher.js",
    "html:watch": "node ./tasks/html-watch.js",
    "lint": "eslint -c .eslintrc src/**",
    "lint:fix": "eslint -c .eslintrc --fix src/**",
    "image": "node ./tasks/image.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yiwenl/webpack-template.git"
  },
